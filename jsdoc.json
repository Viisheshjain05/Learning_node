{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            ".idea/",
            "test/",
            "src/",
            "index.js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules/"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./template/braintree/",
        "encoding": "utf8",
        "recurse": true,
        "destination": "./docs/",
        "readme": "./.idea/JsDoc.md"
    }
}